<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/img2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SightsAndLandmarks"
            android:label="Sights And Landmarks"/>
        <activity android:name=".Museums"
            android:label="Museums"/>
        <activity android:name=".Gardens"
            android:label="Gardens"/>
        <activity android:name=".ShoppingMalls"
            android:label="Shopping Malls"/>

    </application>

</manifest>